<% provide(:title, "Login") %>

<div class="profile-header container">
    <h1>Login</h1>
</div>

<div class="container">
    <%= form_with(url: '/login', scope: :session, local: true) do |f| %>
        <%= f.label :email %>
        <%= f.email_field :email %>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <%= f.submit "Login" %>
    <% end %>
</div> 
